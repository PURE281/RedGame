    speakerName: "\u4F19\u8BA1\u963F\u4E00"
    text: "\u4F19\u8BA1\u963F\u4E00\u62B1\u6B49\u9053\uFF1A\u201C\u4E0D\u597D\u610F\u601D\u54DF\uFF0C\u8FD9\u4F4D\u4ED9\u5B50\uFF0C\u7409\u7403\u5927\u8292\u679C\u6628\u65E5\u5C31\u5356\u5149\u5566\uFF01\u8FD9\u8FB9\u5176\u4ED6\u54C1\u79CD\u7684\u8292\u679C\u4E5F\u4E0D\u9519\uFF0C\u8981\u4E0D\u60A8\u5C1D\u5C1D\uFF1F\u201D"
